def task1():
 n = int(input())
 s = ""
 s1 = ""
 for i in range(1, n + 1):
    s += str(i)
 for i in range(n):
    s1=s1+s[i]
 print(s1)
    
def task2():
    pos=int(input("enter position:"))
    s=""
    n=1
    while len(s)<pos:
        s+=str(n)
        n=n+1
    length=0
    for i in range(1,n):
        new_str=str(i)
        length=length+len(new_str)
        if length>=pos:
            print("number at that position is:",i)
task=int(input())
if(task==1):
 task1()
else:
  task2()
